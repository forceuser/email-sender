{
	"name": "email-sender",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"imports": {
		"#api/": {
			"default": "./src/api/"
		},
		"#src/": {
			"default": "./src/"
		}
	},
	"va-release": {
		"owner": "forceuser"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=production node server.js start",
		"dev": "nodemon index.js start",
		"build": "npm run build:client && npm run build:server",
		"build:client": "vite build --outDir dist/client --ssrManifest",
		"build:server": "vite build --outDir dist/server --ssr src/entry-server.js --minify false --base '/' --target modules && mv ./dist/server/entry-server.js ./dist/server/entry-server.cjs",
		"generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender",
		"debug": "node --inspect-brk server",
		"eslint": "eslint .",
		"eslint-fix": "eslint --fix .",
		"cmd-example": "node --eval \"console.log(globalThis)\" --input-type module"
	},
	"dependencies": {
		"cross-env": "^7.0.3",
		"fastify": "^3.17.0",
		"fastify-cookie": "^5.3.1",
		"fastify-express": "^0.3.2",
		"fastify-file-upload": "^3.0.0",
		"fastify-https-redirect": "^1.0.3",
		"fastify-static": "^4.2.2",
		"find-package-json": "^1.2.0",
		"form-data": "^2.3.3",
		"fs-extra": "^10.0.0",
		"isomorphic-unfetch": "^3.1.0",
		"json-stringify-safe": "^5.0.1",
		"mustache": "^4.2.0",
		"node-fetch": "^2.6.1",
		"nodemailer": "^6.6.1",
		"primeicons": "^4.1.0",
		"primevue": "^3.5.0",
		"safe-flat": "^2.0.2",
		"vite": "^2.3.4",
		"vue": "^3.0.8",
		"vue-client-only": "^2.1.0",
		"vue-eslint-parser": "^7.6.0",
		"vue-router": "^4.0.3",
		"winston": "^3.3.3",
		"yaml": "^1.10.2",
		"yargs": "^16.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@babel/eslint-parser": "^7.14.5",
		"@babel/plugin-proposal-unicode-property-regex": "^7.12.13",
		"@babel/plugin-syntax-top-level-await": "^7.14.5",
		"@babel/plugin-syntax-typescript": "^7.12.13",
		"@babel/preset-env": "^7.14.5",
		"@babel/register": "^7.14.5",
		"@babel/runtime": "^7.14.6",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"@vitejs/plugin-vue": "^1.0.0",
		"@vitejs/plugin-vue-jsx": "^1.1.2",
		"@vue/cli-plugin-babel": "^4.5.13",
		"@vue/compiler-sfc": "^3.0.8",
		"@vue/server-renderer": "^3.0.6",
		"babel-loader": "^8.2.2",
		"compression": "^1.7.4",
		"core-js": "^3.11.2",
		"dep-import-type": "file:./dep-import-type",
		"eslint": "^7.27.0",
		"eslint-plugin-vue": "^7.10.0",
		"express": "^4.17.1",
		"less": "^4.1.1",
		"nodemon": "^2.0.7",
		"regenerator-runtime": "^0.13.7",
		"serve-static": "^1.14.1",
		"typescript": "^4.3.2"
	}
}
